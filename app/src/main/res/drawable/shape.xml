<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item>
    <shape android:shape="rectangle" >
        <corners

            android:radius="400dp" />
        <size
            android:width="200dp"
            android:height="200dp" />

        <gradient
            android:angle="135"
            android:endColor="#01f1fa"
            android:startColor="#0189ff"
            android:type="linear" />


    </shape>
</item>
<item
    android:bottom="5dp"
    android:left="5dp"
    android:right="5dp"
    android:top="5dp">
    <shape android:shape="rectangle" >
        <corners
            android:radius="400dp" />
        <solid android:color="#f5f5f5" />
    </shape>
</item>

    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp">
        <shape android:shape="rectangle" >
            <corners
                android:radius="400dp" />
            <solid android:color="#ffffff" />
        </shape>
    </item>

</layer-list>